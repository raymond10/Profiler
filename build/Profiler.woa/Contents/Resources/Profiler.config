# (c) Cocktail
# Fichier de config de l'application Profiler
# Les parametres definis ici remplacent ceux
# definis dans la table GRHUM.GRHUM_PARAMETRES


ADMIN_MAIL=admin@univ.fr
DOMAIN_NAME=univ.fr
HOST_MAIL=smtp.univ.fr
APP_ID=PROFILERF
APP_ALIAS=Profiler
APP_TITLE=PROFILER
APP_USE_MAIL=YES
APP_USE_GEDFS=NO
DEFAULT_SESSION_TIME_OUT=1000

#* Le URL de l'image "logo" de l'etablissement. Elle est affichee sur la page
#* d'acceuil de l'application et dans la bare de menu
MAIN_LOGO_URL=http://apps.server.univ.fr/WebObjects/Images/logoUTTgaucheLittle.png

#repertoire qui contient les.jasper livres par l'application (celui de l'application)
REP_BASE_JASPER_PATH=/opt/Apple/Local/Library/WebObjects/Applications/Profiler.woa/Contents/Resources/reports/

#repertoire qui contient les.jasper VOTRE BASE LOCALE
REP_BASE_JASPER_PATH_LOCAL=/opt/Apple/Local/Library/WebObjects/Applications/Profiler.woa/Contents/Resources/reports/


#repertoire qui contient les.xls livres par l'application (celui de l'application)
REP_BASE_XLS_PATH=/opt/Apple/Local/Library/WebObjects/Applications/Profiler.woa/Contents/Resources/XLS/

#repertoire qui contient les.xls VOTRE BASE LOCALE
REP_BASE_XLS_PATH_LOCAL=/opt/Apple/Local/Library/WebObjects/Applications/Profiler.woa/Contents/Resources/XLS/

#Config SAUT + Modeles
#SAUT_URL = Adresse de votre serveur SAUT si vous en avez un
SAUT_URL=http://apps.server.univ.fr/cgi-bin/WebObjects/ServAut.woa/wa/
SAUT_ID_TRANSLATION=PROFILER:RHUMUSER,ULOGIN:RHUMUSER,COCKAPP:RHUMUSER,MANGUE:RHUMUSER

NET_NO_PROXY_HOSTS=*.univ.fr

#Identification CAS
#utilisation de l'identification CAS
CAS_USE_CAS=YES
APP_USE_CAS=YES

#Le password dans la table GRHUM.COMPTE_SAMBA doit est synchronisé
APP_USE_SAMBA=YES

# c_structure du groupe des individus administrateurs de l'appli

C_STRUCTURE_ADMIN=2413

# Utilisation du moteur de synchro pour la synchro LDAP du password
USE_SYNCREF_PWD=YES

# Chemin vers le répertoire contenant les tâches Talend
TOS_JOB_CENTER_FOLDER=/SyncRefTOSJobCenter/

#Authorise la supression  de la photo : si TRUE modifier le trigger PHOTO.TRG_PHOTOS_EMPLOYES
CAN_DELETE_PHOTO=FALSE

#Adresse mail SAM de DT pour la perte du mot de passe
MAIL_DT_SAM=admin@univ.fr

#Authorise l'edition du certificat de scolarité pour les étudiants
CAN_PRINT_CERTIF_SCOL=FALSE

#affiche ou pas l'onglet scolarite
SHOW_SCOL=FALSE


#Types telephone interdits en modification
FORBIDDEN_TYPE_TEL=PRF,INT
#Types fax interdits en modification
#FORBIDDEN_TYPE_FAX=
#Types mobile interdits en modification
#FORBIDDEN_TYPE_MOB=
#Types sip interdits en modification
#FORBIDDEN_TYPE_SIP=

#Delai en minutes de validité des demandes de re-initialisation des mot de passe (60 par defaut)
#DELAI_EXPIRE_DEMANDE_INIT_PWD=60

# Phrase d'information sur la procédure de récupération des mots de passe perdus de l'établissement.
# Par defaut : "Le service informatique ne peut vous communiquer de nouveau mot de passe sans avoir vérifié votre identité."
PASSWORD_RECOVERY_INFORMATION=Le service informatique ne peut vous communiquer de nouveau mot de passe sans avoir vérifié votre identité.
