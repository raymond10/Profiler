AucInfosPerso : AjaxUpdateContainer {
	id = "AucInfosPerso";
}

BtValideInfosPerso : CktlAjaxSubmitButton {
	name = "ValideEdit";
	action = save;
	title = "Enregistrer";
	text = "Enregistrer";
	type = "validate";
	enabled = true;
	updateContainerID = "AucInfosPerso";
	showBusyImage = true;
}

BtCancelInfosPerso : CktlAjaxButton {
	action = cancelEdit;
	title = "Annuler";
	text = "Annuler";
	type = "cancel";
	enabled = true;
	updateContainerID = "AucInfosPerso";
	onClickBefore = "confirm('Souhaitez-vous r√©ellement annuler les modifications faites sur les informations personnelles ?')";
	showBusyImage = true;
}

InfosPersoForm : ERXWOForm {
	multipleSubmit = true;
	embedded = true;
}
